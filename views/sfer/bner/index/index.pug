extends ../aalayout/layout

block content
	.container.mt-3
		h2 Brand Partment
		.row.mt-3.bg-light
			include ../../sfer/index/manager
		.row.border.mt-3
			.col-6.col-sm-4.col-lg-3
				a.nav-link(href="/bnBrands")
					img.figure-img.img-fluid.rounded(src="/imgs/brand.jpg", width="100%", alt="brand list")
			.col-6.col-sm-4.col-lg-3
				a.nav-link(href="/bnVendors")
					img.figure-img.img-fluid.rounded(src="/imgs/vendors.jpg", width="100%", alt="vendor list")
			.col-6.col-sm-4.col-lg-3
				a.nav-link(href="/bnSconts")
					img.figure-img.img-fluid.rounded(src="/imgs/scont.jpg", width="100%", alt="scont list")
			.col-6.col-sm-4.col-lg-3
				a.nav-link(href="/bnNations")
					img.figure-img.img-fluid.rounded(src="/imgs/nations.jpg", width="100%", alt="nation list")
			.col-6.col-sm-4.col-lg-3
				a.nav-link(href="/bnBcategs")
					img.figure-img.img-fluid.rounded(src="/imgs/categs.jpg", width="100%", alt="brand Category")
		.row.mt-3
			include ../../sfer/index/order
			include ../../sfer/index/index