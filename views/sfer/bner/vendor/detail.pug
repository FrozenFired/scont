extends ../aalayout/layout

block content
	.bg-secondary.py-5
		.container
			.row
				h2.col-12.text-center
					span.text-dark Supplier: 
					span.text-info #{object.code}
	.container.mt-3
		include ./detail/selfInfo
		include ./detail/mark

		if object.sconts && object.sconts.length > 0
			-let sconts = object.sconts;
			include ./detail/brands
	script(src="/js/scont/vendor/detail.js")