extends ../aalayout/layout

block content
	.contain1
		.row
			h1.col-sm-6
				span Brands [#{count}]
			.col-sm-3.text-right
				span: a.btn.btn-info.btn-lg(href="/bnBrandAdd") Add Brand
			.col-sm-3.text-right
				span: a.btn.btn-info.btn-lg(href="/bnScontAdd") Add Discount
		hr
		.row.bg-light.p-2
			.col-sm-12
				include ./list/filter
		hr
		.row
			.col-sm-12
				include ../../../aaPlugin/pageSlip
			.col-sm-12
				include ./list/selfList
		.row
			.col-sm-8
			.col-sm-4
				include ./list/print
	script(src="/js/responsive.js")