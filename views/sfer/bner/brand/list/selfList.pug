.row
	each item in objects
		- let objBg;
		if item.status == 0
			-objBg ="bg-info";
		else if item.status == 1
			-objBg ="bg-default";
		else if item.status == 2
			-objBg ="bg-secondary";
		.col-12.col-sm-4.mt-2(class="item-id-"+item._id)
			.card.p-2.mt-2
				.row
					h3.text-center.col-12(class=objBg+" item-id-"+item._id) 
						a.text-dark(href="/bnBrand/"+item._id) #{item.code}
						if item.nation
							-nation = item.nation;
							span &nbsp;&nbsp;&nbsp; [#{nation.code}]
					if item.bcateg
						-bcateg = item.bcateg;
						.col-6
							h5 [#{Conf.bcate[bcateg.bcate]}]
						.col-6
							h5 [#{bcateg.code}]
					else
						.col-12
							h5 Category Loss
					.col-12 Material: #{item.matDesp}

					.col-12
						span Suppliers: 
						if item.sconts && item.sconts.length > 0
							-sconts = item.sconts;
							font.text-info(size=5) #{sconts.length}
						else
							span.text-danger 0 &nbsp;
							button.btn.btn-danger.del(type="button", data-urldel="/bnBrandDelAjax", data-id=item._id) delete
					.col-12 
						span Website: 
						a(href=item.website, target="view_window") #{item.website}
					.col-12 UpdateAt: #{moment(item.updateAt).format('YYYY/MM/DD')}
script(src="/js/delete.js")