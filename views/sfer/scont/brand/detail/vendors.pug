if object.sconts && object.sconts.length > 0
	-let sconts = object.sconts;
	hr
	h2 #{sconts.length} - Vendors:
	table.table.table-hover.table-bordered
		thead
			tr
				th Vendor
				th Type
				th contacts
				th ac/sa
				th scont
				th Note
				th status
				th views
		tbody
			each scont in sconts
				- let trBg;
				if scont.status == 0
					-trBg ="bg-info";
				else if scont.status == 1
					-trBg ="bg-default";
				else if scont.status == 2
					-trBg ="bg-success";
				else if scont.status == 3
					-trBg ="bg-warning";
				else if scont.status == 4
					-trBg ="bg-secondary";
				else if scont.status == 5
					-trBg ="bg-danger";
				tr(class=trBg+" scont-id-"+scont._id)
					if scont.vendor
						-vendor = scont.vendor;
						if vendor.status == 4
							-vdBg ="bg-secondary";
						else
							-vdBg ="bg-default";
						td(class=vdBg): a.btn.btn-primary(href="/vendorDetail/"+vendor._id) #{vendor.code}
						td(class=vdBg) #{Conf.vtype[vendor.vtype]}
						td(class=vdBg)
							if vendor.contacts && vendor.contacts.length > 0
								-contacts = vendor.contacts;
								each contact in contacts
									-a;
									ul
										li #{contact.contacter} 
										li #{contact.tel} 
										li #{contact.email}
						td #{vendor.ac} / #{vendor.sa}
					else
						td
						td
						td
					td #{scont.scont}%
					td #{scont.note}
					td #{Conf.stsScont[scont.status]}
					td: a.btn.btn-primary(href="/scontDetail/"+scont._id) view