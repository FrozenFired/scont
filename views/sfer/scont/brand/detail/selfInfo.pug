h2 brand Information:
dl.dl-horizontal
	dt code:
	dd #{object.code}
	dt weight: 
	dd #{object.weight}
	dt Category Second: 
	if object.bcateg && object.bcateg.code
		dd #{object.bcateg.code}
	else
		dd lost
	-a;
	dt matDesp
	dd #{object.matDesp}
	dt website
	dd #{object.website}
	dt webNote
	dd #{object.webNote}
	dt firmName
	dd #{object.firmName}
	dt iva
	dd #{object.iva}
	dt plist
	dd #{object.plist}
	dt plNote
	dd #{object.plNote}
	dt atlas
	dd #{object.atlas}
	dt atNote
	dd #{object.atNote}
	dt pTime
	dd #{object.pTime}
	dt cartace
	dd #{object.cartace}
	dt video
	dd #{object.video}
	dt status
	dd #{Conf.stsBrand[object.status]}
	dt moment
	dd DAY/MONTH/YEAR
	dt createAt:
	dd #{moment(object.createAt).format('DD/MM/YYYY')}
	dt updateAt:
	dd #{moment(object.updateAt).format('DD/MM/YYYY')}
	dt creater:
	if object.creater && object.creater.name
		dd #{object.creater.name}
	else
		dd
	-a;
	dt updater:
	if object.updater && object.updater.name
		dd #{object.updater.name}
	else
		dd
	-a;

a.btn.btn-warning.btn-lg(href="/brandUpdate/"+object._id) update