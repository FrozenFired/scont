table.table.table-hover.table-bordered
	thead
		tr
			th vendor code<br/>Weight(for sort)
			th vtype
			th contacts
			th ac/sa
			th status
			th numvendor
			th createAt <br/>updateAt
			th option
	tbody
		each item in objects
			-let a;
			tr(class="item-id-"+item._id)
				td
					a(href="/vendorDetail/"+item._id) #{item.code}
					span <br/> #{item.weight}
				td #{Conf.vtype[item.vtype]}
				if item.contacts && item.contacts.length > 0
					-a;
					td
						each contact in item.contacts
							-a;
							span #{contact.contacter} #{contact.tel}  #{contact.email} <br/>
				else
					td
				td #{item.ac} / #{item.sa}
				td #{Conf.stsVendor[item.status]}
				if item.sconts
					td #{item.sconts.length}
				else
					td 0
				-a;
				td
					span #{moment(item.createAt).format('MM/DD/YYYY')}
					br
					span #{moment(item.updateAt).format('MM/DD/YYYY')}
				td: button.btn.btn-danger.del(type="button", data-urldel="/vendorDel", data-id=item._id) 删除
script(src="/js/delete.js")