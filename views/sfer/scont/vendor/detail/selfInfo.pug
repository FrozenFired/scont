h2 vendor Information:
dl.dl-horizontal
	dt code:
	dd #{object.code}
	dt weight: 
	dd #{object.weight}
	dt Vendor Type: 
	dd #{Conf.vtype[object.vtype]}

	if object.contacts && object.contacts.length > 0
		each contact in object.contacts
			dt contacter:
			dd #{contact.contacter}
			dt telephone:
			dd #{contact.tel}
			dt email:
			dd #{contact.email}
			br
	-a;
	dt ac:
	dd #{object.ac}
	dt sa:
	dd #{object.sa}
	dt NOTE:
	dd #{object.acsaNote}
	dt freight:
	dd #{object.freight}
	dt NOTE:
	dd #{object.note}

	if object.creater
		dt creater:
		dd #{object.creater.name}
	-a;
	if object.updater
		dt updater:
		dd #{object.updater.name}
	-a;
	dt createAt:
	dd #{moment(object.createAt).format('MM/DD/YYYY')}
	dt updateAt:
	dd #{moment(object.updateAt).format('MM/DD/YYYY')}

	dt status:
	dd #{Conf.stsVendor[object.status]}

	dt weight:
	dd #{object.weight}



a.btn.btn-warning.btn-lg(href="/vendorUpdate/"+object._id) update