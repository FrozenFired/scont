extends ../../aalayout/layout

block content
	.container
		a.btn.btn-info.btn-lg.btn-block(href="/nationAdd") Add Nation
		hr
		table.table.table-hover.table-bordered
			thead
				tr
					th 序号
					th nation code
					th Weight (sort)
					th Origin Name
					th English Name
					th Chinese Name
					th nation telCode
					th brand Numbers
					th 删除
			tbody
				-let i = 0;
				each item in objects
					- i++;
					tr(class="item-id-"+item._id)
						td <strong> #{i} </strong>
						td: a(href="/nationDetail/"+item._id) #{item.code}
						td #{item.weight}
						td #{item.name}
						td #{item.nameEN}
						td #{item.nameCN}
						td #{item.tel}
						td #{item.numbrand}
						td: button.btn.btn-danger.del(type="button", data-urldel="/nationDel", data-id=item._id) 删除
	script(src="/js/delete.js")