table.table.table-hover.table-bordered
	thead
		tr
			th order
			th vendor
			th acsa
			th createAt
			th price
			th paidAt
			th Method
			th status
			th Assegno
			th Picture
			th note
	tbody
		each item in objects
			-paidAt="";
			if item.paidAt
				-paidAt = moment(item.paidAt).format('YYYYMMDD');

			- bgRecent = 'bg-default';
			if paidAt < weekday && paidAt > today
				- bgRecent = 'bg-info';

			-bgStatus = Conf.stsPayBg[item.status];
			tr(class="item-id-"+item._id+" "+bgStatus)
				if item.order
					-order = item.order;
					td(class=bgErr): a.btn.btn-info(href="/odOrderDetail/"+order._id) #{order.order}
					if order.vder
						-vder = order.vder;
						td #{vder.code}
					else
						td
				else
					td(class=bgErr)
					td
				td: a(href="/odPayDetail/"+item._id) #{item.code}
				td 
					if item.createAt
						-a;
						span #{moment(item.createAt).format("MM/DD/YYYY")}
				td 
					if (paidAt == today) && (item.status == 0)
						-a;
						.btn.btn-info #{item.price}
						//- button.btn.btn-info.change(type="button", data-url="/odPayStatus", data-sts=1, data-id=item._id) #{item.price}
					else
						span #{item.price}
				td(class=bgRecent) 
					if item.paidAt
						-a;
						span #{moment(item.paidAt).format("MM/DD/YYYY")}
				td #{Conf.payMethod[item.method]}
				td #{Conf.stsPay[item.status]}
				td #{item.agCode}
				td #{item.picUrl}
				td #{item.note}