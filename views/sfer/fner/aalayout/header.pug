.navbar.navbar-expand-lg.navbar-dark.bg-dark.fixed-top.noprint
	a.navbar-brand(href="/fner"): img(src="/imgs/casaLogo.jpg", width="40")
	button.navbar-toggler(type="button" data-toggle="collapse" data-target="#navbarTogglerDemo02" aria-controls="navbarTogglerDemo02" aria-expanded="false" aria-label="Toggle navigation")
		<span class="navbar-toggler-icon"></span>

	.collapse.navbar-collapse#navbarTogglerDemo02
		ul.navbar-nav.mr-auto.mt-2.mt-lg-0
			li.nav-item.active
				a.nav-link(href="/fnFatturas") Fatturas <span class="sr-only">(current)</span>
			li.nav-item.active
				a.nav-link(href="/fnOrders") Order <span class="sr-only">(current)</span>
			li.nav-item.active
				a.nav-link(href="/fnPays") Pay <span class="sr-only">(current)</span>
			li.nav-item.active
				a.nav-link(href="/fnVders") 供应商 <span class="sr-only">(current)</span>
			li.nav-item.active
				a.nav-link(href="/sfTasks") taskList <span class="sr-only">(current)</span>
		ul.navbar-nav
			li.text-info
				span &nbsp;
				a.btn.btn-info(href="/sferInfo") hello: 
					if crSfer.name && crSfer.name.length > 0
						span #{crSfer.name}
					else
						span #{crSfer.code}
					if crSfer.role
						span &nbsp;(#{Conf.sfRole[crSfer.role]})
					if crSfer.photo
						img(src=Svaddr+crSfer.photo, width="30")
					-a;
			span &nbsp;&nbsp;&nbsp;&nbsp;
			li: a.btn.btn-danger(href="/logout") Logout

.container.noprint
	.row
		div(style="height:56px")