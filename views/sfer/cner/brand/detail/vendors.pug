if object.sconts && object.sconts.length > 0
	-let sconts = object.sconts;
	h2.mt-2 供应商列表: - #{sconts.length}个
	each scont in sconts
		- let scontBg;
		if scont.status == 0
			-scontBg ="bg-info";
		else if scont.status == 1
			-scontBg ="bg-default";
		else if scont.status == 2
			-scontBg ="bg-success";
		else if scont.status == 3
			-scontBg ="bg-warning";
		else if scont.status == 4
			-scontBg ="bg-secondary";
		else if scont.status == 5
			-scontBg ="bg-danger";
		.card.p-2.mt-2
			.row
				.col-12.col-sm-7
					if scont.vendor
						-vendor = scont.vendor;
						if vendor.status == 2
							-vdBg ="bg-secondary";
						else
							-vdBg ="bg-default";
						.row.vendorPart.bg-light(class=vdBg)
							-code ='UNDEFINE';
							if vendor.code
								-code = vendor.code;
							h4.col-12.col-sm-6.text-info #{code}
							h5.col-6.col-sm-4 [#{Conf.vtypeCn[vendor.vtype]}]
							-ac = 'no';
							if vendor.ac
								-ac = vendor.ac;
							-sa = 'no';
							if vendor.sa
								-sa = vendor.sa;
							h5.col-6.col-sm-2.text-right #{ac} / #{sa}
					else
						p lost supplier
				.col-12.col-sm-5.p-2.text-center(class=scontBg)
					.row.scontPart
						h4.col-12.col-sm-4 折扣: #{scont.scont}%
						.col-12.col-sm-4 #{moment(scont.updateAt).format('YY年MM月DD日')}
						.col-12.col-sm-4 税: #{scont.iva}
				.col-12.col-sm-7
				.col-12.col-sm-5
					if scont.note
						-a;
						span 备注: #{scont.note}