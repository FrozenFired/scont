.row
	each item in objects
		- let objBg;
		if item.status == 0
			-objBg ="bg-info";
		else if item.status == 1
			-objBg ="bg-default";
		else if item.status == 2
			-objBg ="bg-secondary";
		.col-12.col-sm-6.col-lg-4.col-xl-3.mt-2(class="item-id-"+item._id)
			.card.p-2.mt-2
				.row
					h5.text-center.col-12(class=objBg+" item-id-"+item._id) 
						a.text-dark(href="/cnBrand/"+item._id) 品牌: #{item.code}
						if item.nation
							-nation = item.nation;
							span &nbsp;&nbsp;&nbsp; [#{nation.code}]
					if item.bcateg
						-bcateg = item.bcateg;
						.col-6
							p [#{Conf.bcate[bcateg.bcate]}]
						.col-6
							p [#{bcateg.code}]
					else
						.col-12
							p 分类数据丢失
					.col-12 材料描述: #{item.matDesp}

					.col-12
						span 供应商数: 
						if item.sconts && item.sconts.length > 0
							-sconts = item.sconts;
							font.text-info(size=5) #{sconts.length}
						else
							span.text-danger 0
					.col-12 
						span 网站: 
						a(href=item.website, target="view_window") #{item.website}
					.col-12 更新时间: #{moment(item.updateAt).format('YYYY/MM/DD')}