table.table.table-hover.table-bordered
	thead
		tr
			th 品牌名
			th 国家 <br/> 公司名
			th 种类1 / 种类2 <br/> 材料描述
			th 网址
			//- th status
			th 供应商数
			th 查看
	tbody
		each item in objects
			- let objBg;
			if item.status == 0
				-objBg ="bg-info";
			else if item.status == 1
				-objBg ="bg-default";
			else if item.status == 2
				-objBg ="bg-secondary";
			tr(class=objBg+" item-id-"+item._id)
				td #{item.code}
				td
					if item.nation
						-a;
						span #{item.nation.code}<br/>#{item.firmName}
				td 
					if item.bcateg
						-a;
						li #{Conf.bcate[item.bcateg.bcate]}
						li #{item.bcateg.code}
					if item. matDesp && item.matDesp.length > 0
						-a;
						span [ #{item.matDesp} ]
				td 
					a(href=item.website, target="view_window") #{item.website}
				//- td #{Conf.stsBrand[item.status]}
				if item.sconts
					td #{item.sconts.length}
				else
					td 0
				-a;
				td
					a.btn.btn-primary(href="/cnBrand/"+item._id) 详情