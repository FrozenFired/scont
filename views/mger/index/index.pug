extends ../aalayout/layout

block content
	.container
		hr
		form(method="post", action="/multySfer", enctype="multipart/form-data")
			.form-group.row.udPhoto
				label.col-md-2.col-form-label(for="uplSfer") 批量上传员工
				.col-md-6
					input#uplSfer.btn.btn-info.btn-block(type="file", name="mulFile")
				.col-md-2
					input.btn.btn-warning(type="submit" value="开始上传")
		hr
		form(method="post", action="/multyNation", enctype="multipart/form-data")
			.form-group.row.udPhoto
				label.col-md-2.col-form-label(for="uplNation") 批量上传国家
				.col-md-6
					input#uplNation.btn.btn-info.btn-block(type="file", name="mulFile")
				.col-md-2
					input.btn.btn-warning(type="submit" value="开始上传")
		hr
		form(method="post", action="/multyBcateg", enctype="multipart/form-data")
			.form-group.row.udPhoto
				label.col-md-2.col-form-label(for="uplBcate") 品牌二级分类
				.col-md-6
					input#uplBcate.btn.btn-info.btn-block(type="file", name="mulFile")
				.col-md-2
					input.btn.btn-warning(type="submit" value="开始上传")