.form-group.row
	label.col-md-2.col-form-label acAt
	label.col-md-1.col-form-label: img#imgAcStart(src="/imgs/clockStart.jpg", width="50px")
	.col-md-4
		input#acStartTime.datepicker.form-control(type="text", name="acStart", placeholder="startDate format:month/day/year", readonly="readonly", value=condAcStart)
	label.col-md-1.col-form-label: img#imgAcEnded(src="/imgs/clockEnd.jpg", width="50px")
	.col-md-4
		input#acEndedTime.datepicker.form-control(type="text", name="acEnded", placeholder="endDate format : month/day/year", readonly="readonly", value=condAcEnded)

.form-group.row
	label.col-md-2.col-form-label saAt
	label.col-md-1.col-form-label: img#imgSaStart(src="/imgs/clockStart.jpg", width="50px")
