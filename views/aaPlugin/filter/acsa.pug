		input#saStartTime.datepicker.form-control(type="text", name="saStart", placeholder="startDate format:month/day/year", readonly="readonly", value=condSaStart)
	label.col-md-1.col-form-label: img#imgSaEnded(src="/imgs/clockEnd.jpg", width="50px")
	.col-md-4
		input#saEndedTime.datepicker.form-control(type="text", name="saEnded", placeholder="endDate format : month/day/year", readonly="readonly", value=condSaEnded)


